      }
    },
    {
      "id": 13,
      "type": "row",
      "title": "Connections & Errors",
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 24 }
    },
    {
      "id": 14,
      "type": "stat",
      "title": "Open Connections",
