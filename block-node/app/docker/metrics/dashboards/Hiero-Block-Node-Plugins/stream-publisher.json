{
  "uid": "stream_publisher_plugin",
  "title": "Stream Publisher Plugin",
  "tags": ["hiero", "publisher"],
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 2,
  "refresh": "30s",
  "graphTooltip": 0,
  "time": {
    "from": "now-3h",
    "to": "now"
  },
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "name": "Annotations & Alerts",
        "uid": "-- Grafana --",
        "type": "dashboard",
        "datasource": {
          "type": "grafana",
          "uid": "-- Grafana --"
        },
        "enable": true,
        "hide": true,
        "iconColor": "rgba(0, 211, 255, 1)"
      }
    ]
  },
  "templating": { "list": [] },
  "links": [],
  "panels": [
    {
      "id": 1,
      "type": "row",
      "title": "Traffic Rates",
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 0 }
    },
    {
      "id": 2,
      "type": "timeseries",
      "title": "Block Items Received /s",
      "datasource": { "uid": "prometheus" },
      "gridPos": { "h": 8, "w": 8, "x": 0, "y": 1 },
      "targets": [
        {
          "expr": "rate(hiero_block_node_publisher_block_items_received_total[$__rate_interval])",
          "refId": "A"
        }
      ],
      "options": { "showPoints": "auto" },
      "fieldConfig": {
        "defaults": { "unit": "ops", "decimals": 0 },
        "overrides": []
      }
    },
    {
      "id": 3,
      "type": "timeseries",
      "title": "Blocks Ack Sent /s",
      "datasource": { "uid": "prometheus" },
      "gridPos": { "h": 8, "w": 8, "x": 8, "y": 1 },
      "targets": [
        {
          "expr": "rate(hiero_block_node_publisher_blocks_ack_sent_total[$__rate_interval])",
          "refId": "A"
        }
      ],
      "options": { "showPoints": "auto" },
      "fieldConfig": {
        "defaults": { "unit": "ops", "decimals": 0 },
        "overrides": []
      }
    },
    {
      "id": 4,
      "type": "timeseries",
      "title": "Blocks Skips Sent /s",
      "datasource": { "uid": "prometheus" },
      "gridPos": { "h": 8, "w": 8, "x": 16, "y": 1 },
